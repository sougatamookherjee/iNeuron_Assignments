    "'hello'\n",
    "-87.8\n",
    "- \n",
    "/ \n",
    "+\t\n",
    "6 \n",
    "\n",
    "Ans;\n",
    "* -->> expression\n",
    "\n",
    "'hello' -->> Values\n",
    "\n",
    "-87.8 -->> Values\n",
    "\n",
    "- -->> expression\n",
    "\n",
    "/ -->> expression\n",
    "\n",
    "+ -->> expression\n",
    "\n",
    "6 -->> Values"
